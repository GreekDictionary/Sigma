<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003"> 
  <Target Name="AfterBuild"> 
    <ItemGroup> 
	<!-- ~version~ is a custom notation, it gets automatically replaced by the build script -->
      <MyPackageFiles Include="$(MSBuildProjectDirectory)\..\Packages\Sigma.Core.Monitors.WPF.~version~\lib\de-DE\*.*"/> 
    </ItemGroup> 
    <Copy SourceFiles="@(MyPackageFiles)" DestinationFolder="$(OutputPath)\de-DE\" > 
    </Copy> 
  </Target> 
</Project>
